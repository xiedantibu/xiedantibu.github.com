<td class="t-dcl-nopad"> <div><span class="mw-geshi cpp source-cpp"><span class="kw1">template</span><span class="sy1">&lt;</span> <span class="kw1">class</span> T <span class="sy1">&gt;</span><br />
<span class="kw4">void</span> swap<span class="br0">&#40;</span> basic_fstream<span class="sy1">&lt;</span>T<span class="sy1">&gt;</span> <span class="sy3">&amp;</span>lhs, basic_fstream<span class="sy1">&lt;</span>T<span class="sy1">&gt;</span> <span class="sy3">&amp;</span>rhs <span class="br0">&#41;</span><span class="sy4">;</span></span></div></td>
