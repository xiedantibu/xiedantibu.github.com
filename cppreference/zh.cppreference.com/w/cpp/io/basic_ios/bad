    <a href="../basic_ifstream"><span class="kw1402">std::<span class="me2">ifstream</span></span></a> file<span class="br0">&#40;</span><span class="st0">&quot;test.txt&quot;</span><span class="br0">&#41;</span><span class="sy4">;</span>
